{
  "name": "white-rabbit-cli",
  "version": "1.1.2",
  "type": "module",
  "description": "A CLI tool to quickly initialize various types of projects.",
  "main": "dist/index.js",
  "bin": {
    "white-rabbit": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && node add-js-extensions.js && npm run minify",
    "clean": "depcheck && npm prune",
    "minify": "node minify.mjs",
    "start": "node dist/index.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "keywords": [
    "cli",
    "project-initialization",
    "typescript",
    "nodejs"
  ],
  "author": "Romain Portanguen",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "execa": "^6.0.0",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.101",
    "glob": "^10.4.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.2",
    "uglify-js": "^3.18.0"
  }
}
